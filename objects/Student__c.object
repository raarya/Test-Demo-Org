    <validationRules>
        <fullName>Account_number_numeric_if_not_blank</fullName>
        <active>true</active>
        <errorConditionFormula>IF
(NOT(ISBLANK( Account_number__c )),
NOT(ISNUMBER( Account_number__c )),
false)</errorConditionFormula>
        <errorDisplayField>Account_number__c</errorDisplayField>
        <errorMessage>account number should be numeric</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOB_not_greater_than_today</fullName>
        <active>true</active>
        <errorConditionFormula>Date_of_Birth__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>DOB should not be greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percentage_rule</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Mix_Pct__c &lt;=0, Mix_Pct__c &gt;=1)</errorConditionFormula>
        <errorDisplayField>Mix_Pct__c</errorDisplayField>
        <errorMessage>This value can onlu be between 0 and 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_code_format_for_USA</fullName>
        <active>true</active>
        <errorConditionFormula>IF
(
OR(ISPICKVAL( Billing_Country__c,&apos;US&apos;),ISPICKVAL( Billing_Country__c,&apos;USA&apos;)), 
NOT
(
OR
(
REGEX( Billing_Zip_Postal_Code__c , &quot;[0-9]{5}-[0-9]{4}&quot;),
REGEX( Billing_Zip_Postal_Code__c , &quot;[0-9]{5}&quot;)
)
),
false
)</errorConditionFormula>
        <errorDisplayField>Billing_Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>The Zip code should be in proper format</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>credit_card_number</fullName>
        <active>true</active>
        <errorConditionFormula>IF
(
NOT(ISBLANK(Credit_Card_Number__c)),
NOT(
OR
(
REGEX(  Credit_Card_Number__c  , &quot;[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{4}&quot;),
REGEX( Credit_Card_Number__c , &quot;[0-9]{16}&quot;)
)),
false
)</errorConditionFormula>
        <errorDisplayField>Credit_Card_Number__c</errorDisplayField>
        <errorMessage>The credit card number should be in proper format</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>multiple_of_5</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(

NOT(ISBLANK( Multiple_of_5__c )),
IF(MOD( Multiple_of_5__c,5)=0,false,true)

)</errorConditionFormula>
        <errorDisplayField>Multiple_of_5__c</errorDisplayField>
        <errorMessage>the value should be a multiple of 5</errorMessage>
    </validationRules>
